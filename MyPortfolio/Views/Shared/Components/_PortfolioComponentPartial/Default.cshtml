                                <img src="@item.ImageUrl"
                                 srcset="/Hola10/images/portfolio/gallery/1.jpg 1x, /Hola10/images/portfolio/gallery/1.jpg 2x" alt="">
                                <span class="shadow-overlay"></span>
                            </a>
                        </div>
                        <div class="item-folio__text">
                            <h3 class="item-folio__title">
                                @item.Title
                            </h3>
                            <p class="item-folio__cat">
                                @item.SubTitle
                            </p>
                        </div>
                        <a href="@item.Url" class="item-folio__project-link" title="Project link">
                            <i class="im im-link"></i>
                        </a>
                        <div class="item-folio__caption">
                            <p>@item.Description</p>
                        </div>
                    </div> <!-- SQL üzerinden tek bir veriyi çektim görsel adresi vb fakat 8 tane görsel için nasıl yapacağım bilmiyorum yapmayı denedim görsel boyutlarında farklılık oldu daha sonra tekrar deneyeceğim -->
